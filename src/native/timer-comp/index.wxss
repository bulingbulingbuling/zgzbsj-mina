.time-sec {
  display: flex;
  align-items: center;
  justify-content: center;
  color: #444444;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}
.time-sec .time-wrapper {
  display: flex;
  align-items: center;
  font-size: 47rpx;
  color: #444444;
  font-weight: bold;
  margin-left: 25rpx;
}
.time-sec .time-wrapper .time-item {
  display: block;
  width: 80rpx;
  height: 62rpx;
  background-color: #fff;
  border-radius: 7rpx;
}
.time-bottom {
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 220rpx;
  height: 51rpx;
  line-height: 51rpx;
  background-color: #ff9891;
  text-align: center;
  font-size: 24rpx;
  letter-spacing: 1rpx;
  color: #303030;
  z-index: 100;
}